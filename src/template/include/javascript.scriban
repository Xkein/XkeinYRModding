func gen_v8_enum(htEnum)
    $enum = $"entt::meta<{htEnum.enum_name}>().type(\"{htEnum.enum_name}\"_hs)"
    $enum += (enum_constants | array.each do
            ret "   " + gen_v8_enum_constant(htEnum, $0)
        end) | array.join ", \n"
    $enum += "; \n"
    ret $enum
end

