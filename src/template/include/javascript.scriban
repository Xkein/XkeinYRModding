func gen_v8_class(htClass)
    $klass = $"xxx"
    for function in class_functions
        $klass += "   " + gen_v8_class_function(htClass, function) + "\n"
    end
    for property in class_properties
        $klass += "   " + gen_v8_class_property(htClass, property) + "\n"
    end
    $klass += ";\n"
    ret $klass
end

